    "deploy:lib": "cd ./dist/blank && npm publish && cd ../../",
    "build:app": "ng build && ngxer generate && ngsw-config ./docs ./ngsw-config.json",
    "deploy:app": "git add . && git commit -m 'deploy:app' && git push",
    "test": "echo 'test'",
    "lint": "echo 'lint'",
    "e2e": "echo 'e2e'"
